{
  "name": "recovery-app",
  "version": "1.1.3",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "node set-git-tag.js",
    "set_version": "[ -z \"$VERSION\" ] && export VERSION=$(cat package.json | jq -r .version); sed -i -e \"s/GIT_TAG: .*$/GIT_TAG: '$VERSION'/g\" src/environments/environment.prod.ts",
    "build:prod": "npm run set_version && ng build --configuration production --aot",
    "build:dev": "npm run set_version && ng build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "@redpesk/afb-ws": "^1.0.1",
    "bootstrap": "^5.2.3",
    "file-saver": "^2.0.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.15",
    "@angular/compiler-cli": "^16.2.12",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5"
  }
}
