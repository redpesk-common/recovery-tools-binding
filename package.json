{
  "name": "recovery-app",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "node set-git-tag.js",
    "build:prod": "ng build --configuration production --aot",
    "build:tar-prod": "VERSION=$(node -p \"require('./package.json').version\") && PREFIX='recovery-tools-binding-' && ng build --configuration production --aot && mkdir -p $PREFIX$VERSION && mv dist/* $PREFIX$VERSION/ && tar -czf $PREFIX$VERSION.tar.gz $PREFIX$VERSION && rm -rf $PREFIX$VERSION",
    "build:tar-dev": "VERSION=$(node -p \"require('./package.json').version\") && PREFIX='recovery-tools-binding-' && ng build && mkdir -p $PREFIX$VERSION && mv dist/* $PREFIX$VERSION/ && tar -czf $PREFIX$VERSION.tar.gz $PREFIX$VERSION && rm -rf $PREFIX$VERSION"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "@redpesk/afb-ws": "^1.0.1",
    "bootstrap": "^5.2.3",
    "file-saver": "^2.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~4.0.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
